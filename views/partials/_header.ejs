<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-info">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <% if(!user) {%>
    <form action="/register" method="get">
      <button type="submit" class="btn btn-primary ml-3">Register</button>
    </form>
    <form action="/login" method="get">
      <button type="submit" class="btn btn-primary ml-3">Signin</button>
    </form>
    <% } else { %>
    <h5 class="text-white"><%= email %></h5>
    <form action="/logout" method="post">
      <button type="submit" class="btn btn-danger ml-3">Log Out</button>
    </form>
    <% } %>
  </nav>
</header>
